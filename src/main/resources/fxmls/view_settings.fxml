<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="173.0" prefWidth="545.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="music.ControllerSettings">
    <children>
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <children>
                <Label text="Music directory: "/>
                <TextField fx:id="txtfDirectory" editable="true" maxWidth="1.7976931348623157E308" prefHeight="31.0" HBox.hgrow="ALWAYS"/>
                <Button onAction="#handleButtonBrowse" mnemonicParsing="false" text="Browse..."/>
            </children>
        </HBox>
        <HBox alignment="BOTTOM_RIGHT" spacing="5.0" VBox.vgrow="ALWAYS">
            <children>
                <Button onAction="#handleButtonSave" mnemonicParsing="false" prefWidth="80.0" text="Save"/>
                <Button onAction="#handleButtonCancel" mnemonicParsing="false" prefWidth="80.0" text="Cancel"/>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </HBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="10.0" right="10.0" top="30.0"/>
    </padding>
</VBox>
