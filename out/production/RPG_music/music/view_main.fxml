<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="music.ButtonsController">
    <children>
        <Pane minHeight="100.0" minWidth="300.0" prefHeight="114.0" prefWidth="300.0">
            <VBox.margin>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
            </VBox.margin>
            <children>
                <Button fx:id="btnPlay" layoutX="10.0" layoutY="11.0" mnemonicParsing="false" onAction="#handleButtonPlay" prefHeight="80.0" prefWidth="80.0" text="PLAY" />
                <Button layoutX="110.0" layoutY="11.0" mnemonicParsing="false" prefHeight="80.0" prefWidth="80.0" text="PAUSE" />
                <Button layoutX="210.0" layoutY="11.0" mnemonicParsing="false" prefHeight="80.0" prefWidth="80.0" text="STOP" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </Pane>
        <GridPane fx:id="gridForButtons" alignment="CENTER" gridLinesVisible="true" hgap="5.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: blue;" vgap="5.0" VBox.vgrow="ALWAYS">
            <padding>
                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </padding>
        </GridPane>
        <ListView minHeight="50.0" minWidth="-Infinity" prefHeight="127.0" prefWidth="300.0" VBox.vgrow="SOMETIMES">
            <VBox.margin>
                <Insets />
            </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
        </ListView>
    </children>
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>
</VBox>
